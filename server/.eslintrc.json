{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": 0,
    "import/no-dynamic-require": 0
  }
}
